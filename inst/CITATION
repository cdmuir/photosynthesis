citHeader("To cite photosynthesis in publications use:")

citEntry(
  entry    = "Misc",
  title    = "photosynthesis: model {C3} photosynthesis",
  author   = c(person("Christopher", "Muir", "D")),
  note     = "R package version 1.0.2",
  year     = "2020",
  url      = "https://CRAN.R-project.org/package=photosynthesis",
  textVersion = paste("Muir CD. (2020).",
                 title = "photosynthesis: model C3 photosynthesis. R package version 1.0.2.",
                 "https://CRAN.R-project.org/package=photosynthesis."

  )
)
